{
    "name": "sheppers/rest-doc-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to document a described RESTful web service",
    "keywords": ["rest", "bundle", "api", "doc"],
    "homepage": "https://github.com/lshepstone/rest-doc-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrance Shepstone",
            "email": "lawrance.shepstone@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "lshepstone/rest-describe-bundle": "*"
    },
    "require-dev": {
        "symfony/symfony": "2.2.*"
    },
    "autoload": {
        "psr-0": { "Sheppers\\RestDocBundle": "src" }
    }
}
